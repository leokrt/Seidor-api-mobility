{
  "name": "typeorm-sample",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^16.18.104",
    "eslint": "^9.8.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.20"
  },
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "test": "jest --coverage",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "typeorm migration:create ./src/migrations/",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate ./src/migrations/$npm_config_name -d ./src/config/data-source.ts",
    "migration:show": "npx typeorm-ts-node-commonjs migration:show -d ./src/config/data-source.ts",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d  ./src/config/data-source.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/config/data-source.ts"
  }
}
